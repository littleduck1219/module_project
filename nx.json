{
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "dev"]
      }
    }
  },
  "projects": {
    "@atto_ui/esbuild_config": {
      "tags": []
    },
    "@atto_ui/themes": {
      "tags": []
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    }
  },
  "implicitDependencies": {
    "@atto_ui/themes": ["@atto_ui/esbuild_config"]
  }
}
